<?xml version="1.0"?>
<launch>
  <!--源文件是ur5_gazebo/launch/ur5.launch-->
  <include file="$(find betago_description)/launch/ur5/controller_utils.launch"/>
  <!--注意！！！如果ur5模型的prefix进行了赋值，这里的控制参数对应的关节名也得改-->
  <rosparam file="$(find betago_description)/controller/arm_controller_ur5.yaml" command="load"/>
  <node name="arm_controller_spawner" pkg="controller_manager" type="controller_manager" args="spawn arm_controller" respawn="false" output="screen"/>

</launch>
